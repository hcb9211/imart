  /* css********************************************** */
page{
  background: #fff;
  font-family: "微软雅黑";
  color: #444
}
.functionality-li{
  width: 100%;
  height: 100rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #eee;
  padding-left: 20rpx;
  padding-right: 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.functionality-li-text{
  font-size: 32rpx;
}
/*****************************************  */
.yimao{
   font-size: 26rpx;
  position: absolute;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.3)
}
.yimaoke{
  width: 460rpx;
  height: auto;
  margin: 250rpx auto;
  box-sizing: border-box;
  background: #eee;
  border-radius:20rpx; 
   font-size: 26rpx;
}
.yimaoke-text{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center; font-size: 26rpx;
}
.yimaoke-input{
  width: 90%;
  height: 70rpx;
  margin: 20rpx auto;
  color: #999999;
  font-size: 22rpx;
  line-height: 70rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #999999;
}
.yimaoke-botton{
  width: 100%;
  height: 70rpx;
  margin-top: 60rpx;
  display: flex;
  justify-content: space-between;
}
.yimaoke-image{
  margin: 30rpx auto;
  width: 200rpx;
  height: 200rpx;
} 
.yimaoke-image image{
  width: 100%;
  height: 100%;
}
.yimaoke-imagetext{
  text-align: center;
  font-size: 26rpx;
  color: #999999
}
.yimaoke-cancel,.yimaoke-confirm{
  width: 50%;
  height: 70rpx;
  text-align: center;
  line-height: 70rpx;
  font-size: 32rpx;
}
.yimaoke-cancel{
  border-bottom-left-radius: 20rpx;
  background-color: #fff;
  color: #444;
}
.yimaoke-cancel:hover{
  background-color: #999999
}
.yimaoke-confirm{
  border-bottom-right-radius: 20rpx;
  background-color: #d02e2e;
  color: #fff;
}
/*手机验证*********************************************  */
.bindingphone{position: absolute;top: -1rpx;left: 0rpx; width: 100%;height: 100%;background: rgba(123, 123, 123, 0.5);}
.bp-content{position: relative; margin: 250rpx auto; width: 520rpx;height: 470rpx;background: #ffffff;border-radius: 20rpx;padding-top: 40rpx; font-family: "微软雅黑";}
.bp-head{margin-bottom: 40rpx;width: 520rpx;height: 60rpx;text-align: center;font-size: 32rpx;color: #444444}
.bp-phonenum{margin: 30rpx auto; width: 460rpx;height: 60rpx;border-bottom: 1rpx solid #bbbbbb;}
.bp-phonenum input{font-size: 28rpx; color: #777777}
.bp-verification{position: relative; margin: 30rpx auto; width: 460rpx;height: 60rpx;}
.bp-verification .bp-verification-inp{width: 290rpx;height: 60rpx;border-bottom: 1rpx solid #bbbbbb}
.bp-verification .bp-verification-inp input{width: 100%;font-size: 28rpx; color: #777777}
.bp-verification .bp-verification-btn{ position: absolute;right: 0rpx;top: 0rpx;  width: 150rpx;height: 60rpx;box-sizing: border-box; border: 2rpx solid #d02e2e;border-radius: 4rpx; font-size: 24rpx; color: #d02e2e;text-align: center;line-height: 60rpx;}
.bp-recommend{margin: 30rpx auto; width: 460rpx;height: 60rpx;font-size: 28rpx;}
.bp-recommend input{width: 460rpx;height: 60rpx;border-bottom: 1rpx solid #bbbbbb}
.bp-button{position: absolute;bottom: -4rpx; width: 100%;height: 80rpx;display: flex;flex-direction: row }
.bp-button view{width: 50%;text-align: center;line-height: 80rpx;}
.bp-button .bp-cancel{background: #ffffff; border-top: 1rpx solid #bbbbbb;border-bottom-left-radius:20rpx;font-size: 30rpx;color: #666666}
.bp-button .bp-confirm{background-color: #d02e2e;border-bottom-right-radius: 20rpx; font-size: 30rpx;color: #ffffff}