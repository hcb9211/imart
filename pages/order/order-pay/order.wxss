@import "../../common/cart.wxss";
@import "../../usercenter/bindingphone/bindingphone.wxss";
@font-face {
	font-family: 'iconfont';
	src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
	font-weight: normal;
	font-style: normal;
}
.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-iconfont45:before { content: "\e71a"; }

.icon-arrow-l:before { content: "\e63f"; }

.icon-jia:before { content: "\e601"; }
page{
 background-color: rgb(246, 246, 246);
}
.cart-content{
  width: 750rpx;
  overflow-x: hidden
}
/* .commodity-soild{} */
.original-price,.discounts,.yimao-wallet,.cash-coupon,.order-all,.order-pay-choose {
  background-color: #fff;
}
.comm-content-number{
  position: relative;
}
/* 样式覆盖 */
.order-number{
  position: absolute;
  top: 60rpx;
  right: -120rpx;
  border: 0rpx;
  color: #666;
}
.order-sum{
  text-align: right;
  line-height: 110rpx;
  font-family: "微软雅黑";
  font-size: 32rpx;
  color: #666;
}
.order-sum text:first-child{
  font-size: 20rpx;
  color: #d02e2e;
}
.order-sum text:last-child{
  margin-right: 20rpx;
  color: #d02e2e;
}
.commodity-soild:last-child{
  margin-bottom: 0rpx;
}
/*后期修改 2017/9/22 */
.original-price,.discounts,.yimao-wallet,.cash-coupon{
  height: 50rpx;
  padding: 20rpx;
  border-top: 1rpx solid #eeeeee;
  line-height: 50rpx;
  display: flex;
  justify-content: space-between;
  font-size: 26rpx;
  color: #666666;
}
.original-price-content-symbol{
  color: #444444;
  font-size: 22rpx;
}
.original-price-content-num{
  color: #444444;
  font-size: 30rpx;
}
.discounts-content-num,.yimao-wallet-content-symbol,.cash-coupon-content-symbol{
  color: #f15454;
}
.yimao-wallet-content-radio{
  margin-left: 20rpx;
  position: relative;
  top: -4rpx;
  left: 6rpx;
}
.icon-arrow-l{
  position: relative;
  top: 4rpx;
  margin-left: 32rpx;
  font-size:22px;
  color:#999999;
}
.comm-content-price{
  position: relative;
  top: -10rpx;
}
.order-pay-choose{
  margin-top: 100rpx;
}
/* 修改的样式（HB） */
.orderlistbox{
  width:100%;
  height: 400rpx;
  margin-bottom:80rpx;
}