.icon-iconfont45:before { content: "\e71a"; }

.icon-jia:before { content: "\e601"; }
/*数量  */
page{background: rgb(246, 246, 246)}
.comm-content-quantity{
  float: right;
  position: relative;
  bottom: 15rpx; 
  margin-right: 60rpx;
  width: 160rpx;
  height: 50rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid rgb(212, 212, 212);
  text-align: center;
}
/*减号  */
.comm-quantity-jian{
  left: -40rpx;
  top: -13rpx;
}
.comm-quantity-num{
  top: -14rpx;
}
/*加号  */
.comm-quantity-jia{
  left: 25rpx;
  top: -17rpx;
}
.comm-neirong{
  width: 730rpx;
  padding-top: 10rpx;
  z-index: 99;
  position: absolute;
  background-color: #ffffff; 
}
.comm-neirong:first-child{
  border: 0rpx;
}
/*删除 按钮 */
.comm-del{
  z-index: 9; 
  position: absolute;
  right: 0;
  width: 170rpx;
   height: 200rpx; 
  background: rgb(244, 129, 36);
  text-align: center;
  line-height: 200rpx;
  
}
.comm-del>image{
  width: 44rpx;
  height: 44rpx;
}
.cart-commodity{
  margin-bottom: 110rpx;
  height: 1000rpx;
}
.touch-item {
  font-size: 14px;
  display: flex;
  justify-content: space-between;
  border-bottom:1px solid #ccc;
  width: 100%;
  overflow: hidden
}

.touch-move-active .content,
.touch-move-active .del {
  -webkit-transform: translateX(0);
  transform: translateX(0);
}